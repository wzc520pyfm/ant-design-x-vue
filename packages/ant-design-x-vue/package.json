{
  "name": "ant-design-x-vue",
  "version": "1.6.0",
  "description": "Craft AI-driven interfaces effortlessly",
  "keywords": [
    "AI",
    "Copilot",
    "ant",
    "antdv",
    "components",
    "framework",
    "chat-ui",
    "vue"
  ],
  "homepage": "https://x-vue.ant.design",
  "bugs": {
    "url": "https://github.com/wzc520pyfm/ant-design-x-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wzc520pyfm/ant-design-x-vue"
  },
  "license": "MIT",
  "author": {
    "name": "wzc520pyfm",
    "email": "wzc520pyf@163.com",
    "url": "https://github.com/wzc520pyfm"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./typings/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./resolver": {
      "types": "./resolver-dist/index.d.ts",
      "import": "./resolver-dist/index.js",
      "require": "./resolver-dist/index.js"
    }
  },
  "main": "./lib/index.js",
  "module": "./es/index.mjs",
  "unpkg": "./dist/index.umd.js",
  "types": "./typings/index.d.ts",
  "files": [
    "es",
    "lib",
    "dist",
    "typings",
    "resolver-dist"
  ],
  "scripts": {
    "build": "run-p build:es build:dist build:resolver",
    "build:es": "vite build && vue-tsc --project ../../tsconfig.build.json",
    "build:dist": "vite build --config ./vite.config.dist.mts",
    "build:resolver": "tsc ../../resolver/index.ts --outDir resolver-dist --declaration",
    "build:watch": "vite build --watch",
    "changelog": "changelogen --release",
    "release": "run-s test changelog build publish",
    "publish": "npm publish && git push --follow-tags",
    "test": "pnpm --filter @ant-design-x-vue/components test"
  },
  "dependencies": {
    "@ant-design/fast-color": "^2.0.6",
    "@emotion/hash": "^0.9.2",
    "@emotion/unitless": "^0.10.0",
    "@vueuse/core": "^13.4.0",
    "classnames": "^2.5.1",
    "csstype": "^3.1.3",
    "stylis": "^4.3.4"
  },
  "devDependencies": {
    "@ant-design-x-vue/components": "workspace:*",
    "@ant-design-x-vue/x-sdk": "workspace:*",
    "@ant-design-x-vue/x-markdown": "workspace:*",
    "@types/node": "^22.10.2",
    "@types/stylis": "^4.2.7",
    "changelogen": "^0.5.7",
    "glob": "^11.0.1",
    "npm-run-all2": "^8.0.3",
    "typescript": "~5.8.0",
    "unplugin-vue-macros": "^2.14.5",
    "vite": "^6.2.0",
    "vue-tsc": "^2.1.6"
  },
  "peerDependencies": {
    "ant-design-vue": ">=4.0.0",
    "vue": ">=3.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
